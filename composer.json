{
    "name": "dyce/pvit-payment",
    "description": "A package to use PVit payment gateway",
    "keywords": ["dyce", "pvit", "payment", "laravel"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dycesystems/pvit-payment/issues",
        "source": "https://github.com/dycesystems/pvit-payment"
    },
    "authors": [
        {
            "name": "DYCE",
            "email": "dev@dyce.net"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dyce\\Pvit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dyce\\Pvit\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
